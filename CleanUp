path=`pwd`
echo $path | grep -Eiq ".*DBC-Chicago/.*/.+"

if [ $? == 0 ]
then
      clear
      tput cup 15 25 
      name=$(echo $path | cut -d / -f 7)
      echo "Clean up before you leave $name" 
      tput cup 0 0
      osascript -e "set Volume 10"
      say "Clean up before you leave $name" &
fi

